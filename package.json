{
  "name": "filmviz",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "react": "^16.2.0",
    "react-dom": "^16.2.0",
    "react-scripts": "1.1.0",
    "rgbquant": "^1.1.2"
  },
  "scripts": {
    "start": "react-scripts start",
    "build": "react-scripts build",
    "lint": "eslint src/**/*.{js,jsx}",
    "test": "react-scripts test --env=jsdom",
    "coverage": "npm test -- --coverage",
    "precommit": "lint-staged",
    "eject": "react-scripts eject"
  },
  "devDependencies": {
    "eslint": "^4.18.1",
    "husky": "^0.14.3",
    "lint-staged": "^6.1.0",
    "prettier": "^1.10.2"
  },
  "prettier": {
    "singleQuote": true,
    "trailingComma": "es5"
  },
  "lint-staged": {
    "src/**/*.{js,jsx,json,css}": [
      "eslint --fix",
      "prettier",
      "git add"
    ]
  }
}
